{
  "name": "graphql-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "apollo-boost": "^0.4.7",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "postinstall": "typesync",
    "start": "react-scripts start",
    "start:with:codegen": "concurrently \"react-scripts start\" \"npm run generate:schema -- --watch\"",
    "generate:schema": "graphql-codegen --config codegen.yml",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.6.1",
    "@graphql-codegen/typescript": "1.9.1",
    "@graphql-codegen/typescript-operations": "1.9.1",
    "@graphql-codegen/typescript-react-apollo": "^1.7.0",
    "@types/concurrently": "^4.1.0",
    "@types/prettier": "^1.19.0",
    "@types/testing-library__react": "^9.1.2",
    "concurrently": "^5.0.2",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.19.1",
    "typesync": "^0.6.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
